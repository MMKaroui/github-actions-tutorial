{
    "family": "your-task-family",
    "networkMode": "awsvpc",
    "containerDefinitions": [{
        "name": "github-actions-tutorial",
        "image": "385320741566.dkr.ecr.us-east-1.amazonaws.com/github-actions-tutorial:latest",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80
        }],
        "essential": true
    }],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::385320741566:role/ecsTaskExecutionRoleNoorGitHub",
    "taskRoleArn": "arn:aws:iam::385320741566:role/ecsTaskRoleNoorGitHub"
}